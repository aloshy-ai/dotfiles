{
    "RainbowBrackets.colorMode": "Consecutive",
    "RainbowBrackets.depreciation-notice": false,
    "cursor.aipreview.enabled": true,
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.cpp.enablePartialAccepts": true,
    "editor.action.moveLinesDownAction": "shift+cmd+down",
    "editor.action.moveLinesUpAction": "shift+cmd+up",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.folding": true,
    "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.semanticHighlighting.enabled": true,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.smartCommitChanges": "all",
    "security.workspace.trust.banner": "always",
    "workbench.activityBar.orientation": "vertical",
    "workbench.sideBar.location": "left",
    "workbench.tree.enableStickyScroll": false,
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "always",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "remoteHub.commitDirectlyWarning": "off",
    "chat.editing.confirmEditRequestRetry": false,
    "git.path": "/opt/homebrew/bin/yadm",
    "git.openRepositoryInParentFolders": "always",
    "terminal.integrated.sendKeybindingsToShell": true,
    "cursor.general.gitGraphIndexing": "enabled",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Dracula Theme",
    "workbench.iconTheme": "easy-icons",
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "markdown.extension.preview.autoShowPreviewToSide": true,
    "terminal.external.linuxExec": "ghostty",
    "cursor.composer.shouldChimeAfterChatFinishes": true
}